{
  "name": "@dvcol/trakt-http-client",
  "type": "module",
  "version": "1.4.3",
  "packageManager": "pnpm@9.1.0",
  "description": "Simple fetch based http client for trakt.tv API with full typescript support (request and response).",
  "homepage": "https://github.com/dvcol/trakt-http-client",
  "bugs": "https://github.com/dvcol/trakt-http-client/issues",
  "keywords": [
    "typescript",
    "library",
    "fetch",
    "http",
    "trakt",
    "API",
    "trakt-http-client"
  ],
  "license": "MIT",
  "author": "dvcol",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvcol/trakt-http-client.git"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">= 8"
  },
  "main": "./dist/clients/index.cjs",
  "module": "./dist/clients/index.js",
  "types": "./dist/clients/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/clients/index.cjs",
      "import": "./dist/clients/index.js",
      "types": "./dist/clients/index.d.ts"
    },
    "./config": {
      "require": "./dist/config/index.cjs",
      "import": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./models": {
      "require": "./dist/models/index.cjs",
      "import": "./dist/models/index.js",
      "types": "./dist/models/index.d.ts"
    },
    "./utils/crypto": {
      "require": "./dist/utils/crypto.utils.cjs",
      "import": "./dist/utils/crypto.utils.js",
      "types": "./dist/utils/crypto.utils.d.ts"
    },
    "./utils/regex": {
      "require": "./dist/utils/regex.utils.cjs",
      "import": "./dist/utils/regex.utils.js",
      "types": "./dist/utils/regex.utils.d.ts"
    },
    "./utils/typescript": {
      "require": "./dist/utils/typescript.utils.cjs",
      "import": "./dist/utils/typescript.utils.js",
      "types": "./dist/utils/typescript.utils.d.ts"
    },
    "./api": {
      "require": "./dist/api/trakt-api.endpoints.cjs",
      "import": "./dist/api/trakt-api.endpoints.js",
      "types": "./dist/api/trakt-api.endpoints.d.ts"
    },
    "./api/minimal": {
      "require": "./dist/api/trakt-api-minimal.endpoints.cjs",
      "import": "./dist/api/trakt-api-minimal.endpoints.js",
      "types": "./dist/api/trakt-api-minimal.endpoints.d.ts"
    },
    "./api/filters": {
      "require": "./dist/api/trakt-api.filters.cjs",
      "import": "./dist/api/trakt-api.filters.js",
      "types": "./dist/api/trakt-api.filters.d.ts"
    },
    "./api/transforms": {
      "require": "./dist/api/trakt-api.transforms.cjs",
      "import": "./dist/api/trakt-api.transforms.js",
      "types": "./dist/api/trakt-api.transforms.d.ts"
    },
    "./api/validators": {
      "require": "./dist/api/trakt-api.validators.cjs",
      "import": "./dist/api/trakt-api.validators.js",
      "types": "./dist/api/trakt-api.validators.d.ts"
    },
    "./api/authentication": {
      "require": "./dist/api/endpoints/authentication.endpoint.cjs",
      "import": "./dist/api/endpoints/authentication.endpoint.js",
      "types": "./dist/api/endpoints/authentication.endpoint.d.ts"
    },
    "./api/calendar": {
      "require": "./dist/api/endpoints/calendar.endpoint.cjs",
      "import": "./dist/api/endpoints/calendar.endpoint.js",
      "types": "./dist/api/endpoints/calendar.endpoint.d.ts"
    },
    "./api/checkin": {
      "require": "./dist/api/endpoints/checkin.endpoint.cjs",
      "import": "./dist/api/endpoints/checkin.endpoint.js",
      "types": "./dist/api/endpoints/checkin.endpoint.d.ts"
    },
    "./api/comments": {
      "require": "./dist/api/endpoints/comments.endpoint.cjs",
      "import": "./dist/api/endpoints/comments.endpoint.js",
      "types": "./dist/api/endpoints/comments.endpoint.d.ts"
    },
    "./api/episodes": {
      "require": "./dist/api/endpoints/episodes.endpoint.cjs",
      "import": "./dist/api/endpoints/episodes.endpoint.js",
      "types": "./dist/api/endpoints/episodes.endpoint.d.ts"
    },
    "./api/lists": {
      "require": "./dist/api/endpoints/lists.endpoint.cjs",
      "import": "./dist/api/endpoints/lists.endpoint.js",
      "types": "./dist/api/endpoints/lists.endpoint.d.ts"
    },
    "./api/movies": {
      "require": "./dist/api/endpoints/movies.endpoint.cjs",
      "import": "./dist/api/endpoints/movies.endpoint.js",
      "types": "./dist/api/endpoints/movies.endpoint.d.ts"
    },
    "./api/notes": {
      "require": "./dist/api/endpoints/notes.endpoint.cjs",
      "import": "./dist/api/endpoints/notes.endpoint.js",
      "types": "./dist/api/endpoints/notes.endpoint.d.ts"
    },
    "./api/people": {
      "require": "./dist/api/endpoints/people.endpoint.cjs",
      "import": "./dist/api/endpoints/people.endpoint.js",
      "types": "./dist/api/endpoints/people.endpoint.d.ts"
    },
    "./api/recommendations": {
      "require": "./dist/api/endpoints/recommendations.endpoint.cjs",
      "import": "./dist/api/endpoints/recommendations.endpoint.js",
      "types": "./dist/api/endpoints/recommendations.endpoint.d.ts"
    },
    "./api/scrobble": {
      "require": "./dist/api/endpoints/scrobble.endpoint.cjs",
      "import": "./dist/api/endpoints/scrobble.endpoint.js",
      "types": "./dist/api/endpoints/scrobble.endpoint.d.ts"
    },
    "./api/search": {
      "require": "./dist/api/endpoints/search.endpoint.cjs",
      "import": "./dist/api/endpoints/search.endpoint.js",
      "types": "./dist/api/endpoints/search.endpoint.d.ts"
    },
    "./api/seasons": {
      "require": "./dist/api/endpoints/seasons.endpoint.cjs",
      "import": "./dist/api/endpoints/seasons.endpoint.js",
      "types": "./dist/api/endpoints/seasons.endpoint.d.ts"
    },
    "./api/shows": {
      "require": "./dist/api/endpoints/shows.endpoint.cjs",
      "import": "./dist/api/endpoints/shows.endpoint.js",
      "types": "./dist/api/endpoints/shows.endpoint.d.ts"
    },
    "./api/sync": {
      "require": "./dist/api/endpoints/sync.endpoint.cjs",
      "import": "./dist/api/endpoints/sync.endpoint.js",
      "types": "./dist/api/endpoints/sync.endpoint.d.ts"
    },
    "./api/users": {
      "require": "./dist/api/endpoints/users.endpoint.cjs",
      "import": "./dist/api/endpoints/users.endpoint.js",
      "types": "./dist/api/endpoints/users.endpoint.d.ts"
    }
  },
  "files": [
    "*.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "tsup",
    "watch": "npm run build -- --watch lib",
    "lint": "eslint lib",
    "lint:fix": "eslint lib --fix",
    "test:unit": "vitest --run --coverage",
    "test:watch": "vitest",
    "release": "standard-version",
    "release:changelog": "extract-changelog-release > RELEASE.md"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {},
  "dependencies": {
    "@dvcol/base-http-client": "^1.10.0",
    "@dvcol/common-utils": "^1.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dvcol/eslint-plugin-presets": "^1.3.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsonc": "^2.15.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.4.0",
    "eslint-plugin-yml": "^1.14.0",
    "extract-changelog-release": "^1.0.2",
    "husky": "^9.0.11",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,svelte,json,md}": [
      "eslint --fix"
    ]
  }
}
